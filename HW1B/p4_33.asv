% define individual numerator transfer functions
N1=tf(10,1);
N2=tf([1 5],1);
% define individual denominator transfer functions
D1=tf(1,[1 2]);
D2=tf(1,[1 8 32]);
% define total transfer function
G=N1*N2*D1*D1*D2;

%
[Y, T] = step(G);
plot(T, Y);
% generate response characteristics: % overshoot, rise time, etc.
S=stepinfo(G);
% generate bode plot showing individual components and total
bode(N1,N2,D1,D1,D2,G);
